.side {
  width: 100vw;
  height: initial;
  position: relative;
  overflow: hidden; }
  .side__view {
    width: calc(100vw + 160rpx);
    height: initial;
    display: flex;
    position: relative; }
  .side__con {
    flex: 1; }
  .side__del {
    position: relative;
    width: 160rpx;
    background: red;
    color: #FFFFFF; }
    .side__del text {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%); }
